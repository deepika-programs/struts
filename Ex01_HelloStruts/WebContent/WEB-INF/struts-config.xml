<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" 
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">

<struts-config>

	<action-mappings>
	
		<action path="/login" type="com.deepika.action.LoginAction" validate="false">
			<forward name="success" path="/WEB-INF/WEB-FILES/good.jsp"></forward>
			<forward name="failure" path="/WEB-INF/WEB-FILES/bad.jsp"></forward>
		</action>
		
	</action-mappings>

</struts-config>